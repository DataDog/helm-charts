

{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "replicaCount": {
        "type": "integer"
      },
      "apiKey": {
        "type": "null"
      },
      "apiKeyExistingSecret": {
        "type": "null"
      },
      "appKey": {
        "type": "null"
      },
      "site": {
        "type": "null"
      },
      "dd_url": {
        "type": "null"
      },
      "env": {
        "type": "array",
        "items": {}
      },
      "appKeyExistingSecret": {
        "type": "null"
      },
      "image": {
        "type": "object",
        "properties": {
          "repository": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          },
          "pullPolicy": {
            "type": "string"
          }
        },
        "required": [
          "repository",
          "tag",
          "pullPolicy"
        ]
      },
      "imagePullSecrets": {
        "type": "array",
        "items": {}
      },
      "nameOverride": {
        "type": "string"
      },
      "fullnameOverride": {
        "type": "string"
      },
      "logLevel": {
        "type": "string"
      },
      "maximumGoroutines": {
        "type": "null"
      },
      "supportExtendedDaemonset": {
        "type": "string"
      },
      "metricsPort": {
        "type": "integer"
      },
      "secretBackend": {
        "type": "object",
        "properties": {
          "command": {
            "type": "string"
          },
          "arguments": {
            "type": "string"
          }
        },
        "required": [
          "command",
          "arguments"
        ]
      },
      "datadogAgent": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean"
          }
        },
        "required": [
          "enabled"
        ]
      },
      "datadogMonitor": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean"
          }
        },
        "required": [
          "enabled"
        ]
      },
      "rbac": {
        "type": "object",
        "properties": {
          "create": {
            "type": "boolean"
          }
        },
        "required": [
          "create"
        ]
      },
      "serviceAccount": {
        "type": "object",
        "properties": {
          "create": {
            "type": "boolean"
          },
          "name": {
            "type": "null"
          },
          "annotations": {
            "type": "object"
          }
        },
        "required": [
          "create",
          "name",
          "annotations"
        ]
      },
      "resources": {
        "type": "object"
      },
      "nodeSelector": {
        "type": "object"
      },
      "tolerations": {
        "type": "array",
        "items": {}
      },
      "affinity": {
        "type": "object"
      },
      "installCRDs": {
        "type": "boolean"
      },
      "datadogCRDs": {
        "type": "object",
        "properties": {
          "crds": {
            "type": "object",
            "properties": {
              "datadogAgents": {
                "type": "boolean"
              },
              "datadogMetrics": {
                "type": "boolean"
              },
              "datadogMonitors": {
                "type": "boolean"
              }
            },
            "required": [
              "datadogAgents",
              "datadogMetrics",
              "datadogMonitors"
            ]
          },
          "migration": {
            "type": "object",
            "properties": {
              "datadogAgents": {
                "type": "object",
                "properties": {
                  "conversionWebhook": {
                    "type": "object",
                    "properties": {
                      "enabled": {
                        "type": "boolean"
                      },
                      "name": {
                        "type": "string"
                      },
                      "namespace": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "enabled",
                      "name",
                      "namespace"
                    ]
                  },
                  "useCertManager": {
                    "type": "boolean"
                  },
                  "version": {
                    "type": "string"
                  }
                },
                "required": [
                  "conversionWebhook",
                  "useCertManager",
                  "version"
                ]
              }
            },
            "required": [
              "datadogAgents"
            ]
          }
        },
        "required": [
          "crds",
          "migration"
        ]
      },
      "podAnnotations": {
        "type": "object"
      },
      "podLabels": {
        "type": "object"
      },
      "collectOperatorMetrics": {
        "type": "boolean"
      },
      "watchNamespaces": {
        "type": "array",
        "items": {}
      },
      "containerSecurityContext": {
        "type": "object"
      }
    },
    "required": [
      "replicaCount",
      "apiKey",
      "apiKeyExistingSecret",
      "appKey",
      "site",
      "dd_url",
      "env",
      "appKeyExistingSecret",
      "image",
      "imagePullSecrets",
      "nameOverride",
      "fullnameOverride",
      "logLevel",
      "maximumGoroutines",
      "supportExtendedDaemonset",
      "metricsPort",
      "secretBackend",
      "datadogAgent",
      "datadogMonitor",
      "rbac",
      "serviceAccount",
      "resources",
      "nodeSelector",
      "tolerations",
      "affinity",
      "installCRDs",
      "datadogCRDs",
      "podAnnotations",
      "podLabels",
      "collectOperatorMetrics",
      "watchNamespaces",
      "containerSecurityContext"
    ]
  }
  
  